{
  "name": "lottie-frame",
  "version": "1.0.4",
  "description": "Convert Lottie animations to PNG frames using Node.js and C++",
  "main": "lib/cjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "gypfile": true,
  "license": "MIT",
  "author": {
    "name": "sanyok12345",
    "email": "justsanyok12345@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanyok12345/lottie-frame.git"
  },
  "files": [
    "lib",
    "src",
    "binding.gyp"
  ],
  "keywords": [
    "lottie",
    "animation",
    "png",
    "frame",
    "convert"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module ESNext --outDir lib",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "prebuild": "OPENSSL_FIPS='' prebuild --runtime napi --target 6 --target 7 --target 8 --target 9 --strip",
    "prepublishOnly": "npm run build && npm run prebuild",
    "install": "prebuild-install || node-gyp rebuild"
  },
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "prebuild-install": "^7.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "node-gyp": "^11.1.0",
    "prebuild": "^13.0.1",
    "typescript": "^5.8.2"
  },
  "binary": {
    "napi_versions": [
      6,
      7,
      8,
      9
    ]
  }
}
